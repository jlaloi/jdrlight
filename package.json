{
  "name": "jdrlight",
  "version": "1.0.0",
  "description": "",
  "main": "./app/index.ts",
  "scripts": {
    "dev": "parcel watch ./src/index.html --out-dir ./public",
    "prod": "parcel build ./src/index.html --out-dir ./public",
    "lint": "eslint --ext .js,.vue --fix src",
    "start": "node ./dist/index.js",
    "start:dev": "node --inspect=5858 -r ts-node/register ./app/index.ts",
    "start:watch": "nodemon",
    "build": "tsc"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "app"
    ],
    "exec": "npm run start:dev",
    "ext": "ts"
  },
  "author": "julien.laloi@gmail.com",
  "license": "ISC",
  "dependencies": {
    "apollo-boost": "^0.3.1",
    "castv2-player": "^2.1.0",
    "fastify": "^2.12.0",
    "fastify-static": "^2.6.0",
    "graphql": "^14.5.8",
    "material-design-icons": "^3.0.1",
    "node-yeelight-wifi": "^0.1.4",
    "request": "^2.88.0",
    "request-promise": "^4.2.5",
    "vue": "^2.6.10",
    "vue-apollo": "^3.0.0",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.1",
    "youtube-player": "^5.5.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.1",
    "@types/compression": "^1.0.1",
    "@types/express": "^4.17.2",
    "@types/node": "^12.12.7",
    "@types/request": "^2.48.3",
    "@types/request-promise": "^4.1.44",
    "@vue/component-compiler-utils": "^3.0.2",
    "babel-core": "^6.26.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-vue": "^5.2.3",
    "nodemon": "^1.19.4",
    "parcel-bundler": "^1.12.4",
    "sass": "^1.23.3",
    "ts-node": "^8.4.1",
    "typescript": "^3.7.5",
    "vue-hot-reload-api": "^2.3.4",
    "vue-resource": "^1.5.1",
    "vue-template-compiler": "^2.6.10"
  }
